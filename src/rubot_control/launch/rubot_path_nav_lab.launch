<launch>
 <!-- Navigation Program   -->
    <arg name="v" default="0.3"/>
    <arg name="w" default="0.4"/>
    <arg name="td" default="2"/>
    <arg name="path" default="Pentagon"/>
  <node name="rubot_nav" pkg="rubot_control" type="rubot_path_nav_lab.py" output="screen" >
    <param name="v" value="$(arg v)"/>
    <param name="w" value="$(arg w)"/>
    <param name="td" value="$(arg td)"/>
    <param name="path" value="$(arg path)"/>
  </node>
</launch>
